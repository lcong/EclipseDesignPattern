#ifndef IHUMAN_H_
#define IHUMAN_H_

#pragma once
class IHuman
{
public:
	IHuman(void)
	{
	}
	virtual ~IHuman(void)
	{
	}
	virtual void Laugh() = 0;
	virtual void Cry() = 0;
	virtual void Talk() = 0;
};

#endif /* IHUMAN_H_ */
